{
  "name": "start-template-test",
  "version": "3.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^18.18 || ^20.9"
  },
  "browserslist": [
    "last 2 versions",
    "not dead",
    "not op_mini all",
    "not and_uc > 0",
    "not < 0.2%"
  ],
  "scripts": {
    "start": "gulp runDev",
    "build": "gulp buildProd",
    "preview": "npm run buildProd && browser-sync start -s build/ --cors --no-notify --no-ui",
    "prelint": "gulp css --silent",
    "lint": "npm-run-all -pc lint:**",
    "lint:spaces": "editorconfig-cli",
    "lint:markup": "cd build && html-validator --quiet",
    "lint:html": "linthtml build/**/*.html --config .linthtmlrc",
    "lint:bem": "gulp lintBem --silent",
    "lint:styles": "stylelint \"source/scss/**/*.scss\"",
    "lint:scripts": "eslint . --ignore-path .gitignore",
    "optimize": "npm-run-all -p optimize:**",
    "optimize:raster": "gulp optimizeRaster",
    "optimize:vector": "gulp optimizeVector"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@htmlacademy/editorconfig-cli": "2.0.8",
    "@linthtml/linthtml": "0.9.5",
    "autoprefixer": "10.4.16",
    "babel-loader": "^9.1.3",
    "browser-sync": "2.29.3",
    "browserslist-to-esbuild": "1.2.0",
    "circular-dependency-plugin": "^5.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.8.1",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "8.55.0",
    "eslint-config-htmlacademy": "10.0.1",
    "gulp": "4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-esbuild": "0.12.0",
    "gulp-file-include": "^2.3.0",
    "gulp-html-beautify": "^1.0.1",
    "gulp-html-bemlinter": "3.0.1",
    "gulp-htmlmin": "5.0.1",
    "gulp-nunjucks": "6.0.0",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "9.0.1",
    "gulp-sass": "5.1.0",
    "gulp-sharp-responsive": "0.4.1",
    "gulp-stacksvg": "3.0.0",
    "gulp-svgmin": "4.1.0",
    "linthtml-config-htmlacademy": "^1.0.14",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.32",
    "postcss-csso": "6.0.1",
    "postcss-url": "10.1.3",
    "sass": "1.69.5",
    "style-loader": "^3.3.3",
    "stylelint": "^16.1.0",
    "stylelint-config-htmlacademy": "^4.0.0",
    "terser-webpack-plugin": "^5.3.9",
    "w3c-html-validator": "1.6.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-stream": "^7.0.0"
  }
}
